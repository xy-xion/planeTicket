{
  "name": "test2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.23.0",
    "echarts": "^4.9.0",
    "file-loader": "^6.2.0",
    "less": "^4.1.2",
    "less-loader": "^5.0.0",
    "url-loader": "^4.1.1",
    "core-js": "^3.6.5",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "vue-template-compiler": "^2.6.11",
    "css-loader": "^6.4.0",
    "style-loader": "^3.3.0"
  },
  "eslintConfig": {
    "root": false,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-console": "off",
      "generator-star-spacing": "off",
      "indent": 0,
      "no-trailing-spaces": 0,
      "comma-dangle": 0,
      "no-multiple-empty-lines": 0,
      "no-unused-vars": 0,
      "padded-blocks": "off",
      "no-useless-escape": 0,
      "no-tabs": "off",
      "no-eval": "off",
      "eqeqeq": 0,
      "handle-callback-err": 0,
      "standard/no-callback-literal": 0,
      "no-undef": 0,
      "no-unused-components": 0,
      "no-empty": 0,
      "no-debugger": 0,
      "no-extra-semi": 0,
      "no-case-declarations": 0
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
